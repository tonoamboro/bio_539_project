file = {:Users/r.tonoamboro/Library/Application Support/Mendeley Desktop/Downloaded/Wujdi, Sulistyaningsih, Rochman - 2015 - Sebaran Hasil Tangkapan Madidihang (Thunnus albacares Bonnaterre, 1788) di Samudera Hindia Bagi.pdf:pdf},
