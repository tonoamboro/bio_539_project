isbn = {0090-1830},
issn = {0090-1830, 0090-1830},
journal = {Marine Fisheries Review},
keywords = {AN,ASFA 3: Aquatic Pollution {\&} Environmental Quality,ASW,Caribbean Sea,Catch/effort,Environmental protection,Florida,Istiophorus platypterus,Makaira nigricans,Man-induced effects,Marine,Marine fish,Marine fisheries,Mortality causes,North Atlantic,O 4090:Conservation and Environmental Protection,Q1 08604:Stock assessment and management,Q5 08501:General,Seasonality,Stock assessment,Tetrapturus albidus,Thunnus,Tracking,USA},
