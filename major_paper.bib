issn = {2502-6542},
journal = {Jurnal Penelitian Perikanan Indonesia},
number = {3},
pages = {173},
title = {{Dinamika Perubahan Metode Penangkapan Rawai Tuna Di Samudera Hindia}},
url = {http://ejournal-balitbang.kkp.go.id/index.php/jppi/article/view/1639},
