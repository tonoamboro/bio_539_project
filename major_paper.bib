file = {:Users/r.tonoamboro/Library/Application Support/Mendeley Desktop/Downloaded/Zhu et al. - 2008 - Length-frequency Compositions and Weight-length Relations for Big-eye Tuna, Yellowfin Tuna, and Albacore (Perciforme.pdf:pdf},
journal = {Acta Ichthyologica et Piscatoria},
number = {2},
pages = {157--161},
title = {{Length-frequency compositions and weight-length relations for big-eye tuna, yellowfin tuna, and albacore (Perciformes: Scombrinae) in the Atlantic, Indian, and eastern Pacific oceans}},
url = {http://www.aiep.pl/volumes/2000/8{\_}2/pdf/12{\_}862{\_}FULLTEXT{\_}Zhu{\_}et{\_}al{\_}PROOF{\_}3.pdf},
